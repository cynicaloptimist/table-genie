{
  "name": "skill-table-genie",
  "version": "1.1.0",
  "description": "Generate random die rolls, names, et cetera.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cynicaloptimist/table-genie.git"
  },
  "scripts": {
    "test": "lambda-local -l ./dist/index.js -e sample_dice.js",
    "test:name": "lambda-local -l ./dist/index.js -e sample_name.js",
    "test:trinket": "lambda-local -l ./dist/index.js -e sample_trinket.js",
    "build": "tsc && copy-node-modules ./ ./dist/ && rm -r ./dist/node_modules/aws-sdk"
  },
  "keywords": [
    "alexa",
    "skill",
    "generator"
  ],
  "author": "Evan Bailey",
  "license": "MIT",
  "dependencies": {
    "ask-sdk": "^2.0.6",
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/jest": "^23.1.1",
    "@types/lodash": "^4.14.110",
    "copy-node-modules": "^1.0.6",
    "jest": "^23.1.0",
    "lambda-local": "^1.4.2",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
