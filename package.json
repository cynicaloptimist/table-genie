{
  "name": "skill-table-genie",
  "version": "1.0.0",
  "description": "Generate random die rolls, names, et cetera.",
  "main": "index.js",
  "scripts": {
    "test": "lambda-local -l index.js -e sample_dice.js",
    "test:name": "lambda-local -l index.js -e sample_name.js"
  },
  "keywords": [
    "alexa",
    "skill",
    "generator"
  ],
  "author": "Evan Bailey",
  "license": "MIT",
  "dependencies": {
    "@types/alexa-sdk": "^1.1.1",
    "@types/jest": "^23.1.1",
    "@types/lodash": "^4.14.109",
    "alexa-sdk": "^1.0.25",
    "axios": "^0.18.0",
    "jest": "^23.1.0",
    "lodash": "^4.17.10",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "lambda-local": "^1.4.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
